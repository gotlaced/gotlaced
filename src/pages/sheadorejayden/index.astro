<script>
  async function incrementViewCountOnce() {
    if (!localStorage.getItem("viewCounted")) {
      try {
        await fetch("https://viewcounter.keplarpayments.workers.dev/api/views", {
          method: "POST"
        });
        localStorage.setItem("viewCounted", "true");
      } catch (e) {
        console.error("Failed to increment view count", e);
      }
    }
  }

  async function fetchViewCount() {
    try {
      const response = await fetch("https://viewcounter.keplarpayments.workers.dev/api/views");
      const data = await response.json();
      document.getElementById("view-number").textContent = data.views.toLocaleString();
    } catch (error) {
      console.error("Failed to fetch views:", error);
      document.getElementById("view-number").textContent = "N/A";
    }
  }

  document.addEventListener("DOMContentLoaded", async () => {
    await incrementViewCountOnce();
    fetchViewCount();
  });

  // Theme toggle button text
  const themeToggleBtn = document.querySelector(".theme-toggle");
  themeToggleBtn.addEventListener("click", () => {
    toggleTheme();
    // Update button text
    if (document.body.classList.contains("hello-kitty")) {
      themeToggleBtn.textContent = "Girly Theme:3 (On)";
    } else {
      themeToggleBtn.textContent = "Girly Theme:3";
    }
  });

  // Mute button functionality
  const muteBtn = document.getElementById("muteBtn");
  muteBtn.addEventListener("click", () => {
    toggleMute();
  });
</script>
